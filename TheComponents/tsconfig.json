{
  "compilerOptions": {
    "strictNullChecks": true,
    "jsx": "react",
    "target": "ES5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": false,
    "module": "ES6",
    "moduleResolution": "node",
    "strict": true,
    "declaration": true,
    "outDir": "dist",
    "sourceMap": true,
    "allowSyntheticDefaultImports" : true,
    "typeRoots": ["./typings", "./node_modules/@types"]
  },
}
